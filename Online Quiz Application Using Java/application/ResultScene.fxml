<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.ResultScene">
moh
    <VBox alignment="CENTER" spacing="20" layoutX="100.0" layoutY="60.0" prefWidth="400" prefHeight="300">
        <Label fx:id="congratsLabel" styleClass="title-label"/>
        <Label fx:id="scoreLabel" styleClass="score-label"/>
        <Button text="Restart Quiz" onAction="#handleRestart"/>
        <Button text="Exit" onAction="#handleExit"/>
    </VBox>
</AnchorPane>
